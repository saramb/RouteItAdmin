        </activity>
        <activity
            android:name=".DeleteBusStation"
            android:label="Delete Bus Station">

        </activity>
        <activity
            android:name=".DeleteMetroStation"
            android:label="Delete Metro Station">

        </activity>
        <activity
            android:name=".EnterBusStation"
            android:label="Enter Bus Station">

        </activity>
        <activity
            android:name=".EnterMetroStation"
            android:label="Enter Metro Station">

        </activity>
